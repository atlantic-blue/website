{
  "scripts": {
    "build": "npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack --env NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "build:analyze": "npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack --env ANALYZE --env NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "clean": "rm -rf dist **/*.scss.d.ts",
    "dev:server": "TS_NODE_PROJECT=tsconfig.json webpack-dev-server --env NODE_ENV=development --config ./config/webpack/webpack.config.ts",
    "doc": "typedoc",
    "lint": "eslint --ext .tsx --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run types:sass:watch | npm run dev:server",
    "storybook:start": "npm run types:sass:watch | start-storybook -p 6006",
    "storybook:build": "npm run types:sass && build-storybook",
    "test": "jest",
    "test:watch": "jest --watch --no-coverage",
    "test:coverage": "jest --coverage",
    "types:sass": "npm run clean && typed-scss-modules src --camelCase",
    "types:sass:watch": "npm run types:sass -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@storybook/addon-actions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addons": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@testing-library/react": "^14.1.2",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/jest": "^29.5.11",
    "@types/mixpanel-browser": "^2.48.1",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "nyc": "^15.1.0",
    "prettier": "^3.1.1",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.3",
    "ts-node": "^10.9.2",
    "typed-scss-modules": "^8.0.0",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "mixpanel-browser": "^2.48.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.7"
  },
  "name": "app",
  "version": "0.0.0"
}
